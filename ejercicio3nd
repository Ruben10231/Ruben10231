public static void main(String[] args) {
        // numero 2
        int x = 1, y = 2, z = 3;
        if (x < z) {
            System.out.println("*");
        } else if (x == z) {
            System.out.println("&");
        } else {
            System.out.println("$");
        }
        // *
    }

}
