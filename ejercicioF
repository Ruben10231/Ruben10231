public static void main(String[] args) {
        // numero 2
        int a1 = 100;
        int a2 = 200;
        //validar
        if (a1 > 100 && a2 <= 200) {
            System.out.println(a1 + " " + a2 + " " + (a1 + a2));
        } else {
            System.out.println("ok");
            System.out.println(a1 + " " + a2 + " " + (2 * a1 - a2));
        }

    }

}
