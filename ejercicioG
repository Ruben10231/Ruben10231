public static void main(String[] args) {
        // numero 2
        int x = 1, y = 2, z = 3;
        if (++x > y++ || x-- > 0) { // x=x+1
            z++;
            System.out.println("r1"); //ok
        } else {
            z--;
            System.out.println("r2");
        }
        System.out.println(x + " " + y + " " + z);
        // 1 3 4
    }

}
